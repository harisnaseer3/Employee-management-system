{
    "manifest_version": 3,
    "name": "Whatsapp Extension",
    "version": "1.3.11",
    "description": "whatsapp extension to send messages",
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "storage",
        "activeTab"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/images/logo.png"
        }
    },

    "icons": {
        "128": "/images/logo1.png"
    }
}
